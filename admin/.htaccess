RewriteEngine On

# Redirect /admin to login.php with the pages=login parameter
RewriteRule ^$ /admin/login.php?pages=login [L,QSA]


RewriteEngine On

# Rewrite /dashboard to /pages/dashboard/dashboard.php?page=dashboard internally
RewriteRule ^dashboard$ /pages/dashboard/dashboard.php?page=dashboard [L,QSA]